{
  "pythonVersion": "3.10",
  "pythonPlatform": "Windows",
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "exclude": [
    "**/tests/**",
    "**/test/**",
    "**/test_*.py",
    "**/*_test.py",
    "**/.venv/**",
    "**/venv/**",
    "**/analysis_reports/**",
    "**/_deprecated/**",
    "**/examples/**",
    "**/docs/**",
    "**/scripts/**",
    "**/benchmarks/**",
    "**/fix_*.py",
    "**/validate_*.py",
    "**/update_*.py",
    "**/migrate_*.py",
    "**/cleanup_*.py",
    "**/analyze_*.py",
    "**/get-pip.py",
    "**/locustfile.py",
    "**/.mypy_cache/**",
    "**/.pytest_cache/**",
    "**/build/**",
    "**/dist/**"
  ],
  "strict": [
    "resync/fastapi_app/**",
    "config/**",
    "resync/core/**",
    "resync/api/**",
    "resync/services/**",
    "resync/cqrs/**",
    "resync/models/**",
    "resync/nlp/**",
    "resync/utils/**",
    "middleware/**"
  ],
  "reportMissingTypeStubs": "warning",
  "strictListInference": false,
  "strictDictionaryInference": false,
  "strictParameterNoneValue": false,
  "strictSetInference": false
}
